<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Title>Passive Aggressive Annotation Framework</Title>
        <Authors>Simon Prinz</Authors>
        <Description>The Passive Aggressive Annotation Framework (PAAF) provides a set of type safe annotations that developers can use to express passive-agressive statements; the framework is provided as a type safe alternative to comments.</Description>
        <Copyright>Simon Prinz</Copyright>
        <PackageProjectUrl>https://github.com/SimonPrinz/Passive-Agressive-Annotation-Framework</PackageProjectUrl>
        <RepositoryUrl>https://github.com/SimonPrinz/Passive-Agressive-Annotation-Framework</RepositoryUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>

</Project>
